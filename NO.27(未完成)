class Solution 
{
public:
    int removeElement(vector<int>& nums, int val) 
    {
     int z=0;                                      
     for(int i=0;i<nums.size();++i)
     {
       if(nums[i]!=val) 
       {
         nums[z]=nums[i];
         ++z;
       }
     }
    return z; 
    }
};

如果把条件换成当nums[]=val
